               <div class="collapsible-body" id="destinations">
               </div>
             </li>
           </ul>
      </div>

      <div class="col s6 m6 l6">
         <div class="divider section"></div>
      </div>
      <div class="row">
      <div class="col s3 m3 l3">
         <ul class="collapsible" data-collapsible="expandable">
             <li>
               <div class="collapsible-header">Suggestions</div>
               <div class="collapsible-body" id="suggestions">
                   <div class="row">
                     <div class="col s12">
                        <ul class="tabs">
                          <li class="tab col s4"><a href="#" class="active">All</a></li>
                          <li class="tab col s4"><a href="#">Bookmarked</a></li>
                          <li class="tab col s4"><a href="#">Added to Route</a></li>
                        </ul>
                     </div>
                     <div id="filtered"></div>
                     <div id="bookmarked"></div>
                     <div id="added-to-route"></div>
                  </div>
               </div>
             </li>
         </ul>
      </div>
   </div>
